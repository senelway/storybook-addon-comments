{
  "presets": [
    "@babel/preset-react",
    ["@babel/env", {
      "modules": false,
      "targets": { "node": 6 }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "babel-plugin-rewire-exports",
    ["@babel/plugin-transform-runtime", {
      "helpers": true,
      "regenerator": true
    }]
  ]
}
